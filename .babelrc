{
  "presets": [
    "react",
    "stage-1"
  ],
  "env": {
    "development": {
      "presets": [
        "latest",
        "react-hmre"
      ]
    },
    "production": {
      "presets": [
        ["latest"]
      ],
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    },
    "test": {
      "presets": [
        "latest"
      ],
      "plugins": [["istanbul", {
        "exclude": [
          "**/*.spec.js"
        ],
        "include": [
          "src/components/**/*.js",
          "src/reducers/**/*.js"
        ]
      }]]
    }
  }
}

